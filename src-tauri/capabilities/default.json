{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "os:default",
    "log:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "dialog:default",
    "deep-link:default",
    "shell:default",
    "fs:allow-resource-read",
    "fs:allow-resource-write",
    "fs:allow-config-write",
    "fs:allow-config-read",
    "fs:allow-data-read",
    "fs:allow-data-write",
    "fs:allow-create",
    "http:default"
  ]
}