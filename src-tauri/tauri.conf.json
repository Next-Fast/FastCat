{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fastcat",
  "version": "0.1.0",
  "identifier": "cat.fast.next",
  "build": {
    "beforeDevCommand": "yarn tarui:pre:dev",
    "devUrl": "http://localhost:2500",
    "beforeBuildCommand": "yarn tarui:pre:build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fastcat",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
